{"ast":null,"code":"import * as THREE from 'three';\nimport { OrbitControls } from 'three/examples/jsm/controls/OrbitControls.js';\nimport { PointLight } from 'three';\nimport { GLTFLoader } from 'three/examples/jsm/loaders/GLTFLoader.js';\nexport default {\n  name: 'ThreeComponent',\n  mounted() {\n    const container = this.$refs.threeContainer;\n    if (!container) {\n      console.error('Element with ref \"threeContainer\" not found');\n      return;\n    }\n    const renderer = new THREE.WebGLRenderer();\n    renderer.setSize(window.innerWidth, window.innerHeight);\n    container.appendChild(renderer.domElement);\n    const scene = new THREE.Scene();\n    scene.background = new THREE.Color(0xdddddd);\n    const camera = new THREE.PerspectiveCamera(45, window.innerWidth / window.innerHeight, 1, 2000);\n    camera.rotation.y = 45 / 100 * Math.PI;\n    camera.position.x = 800;\n    camera.position.y = 100;\n    camera.position.z = 1000;\n    const controls = new OrbitControls(camera, renderer.domElement);\n    controls.update();\n\n    // Adicionando luzes à cena\n    const light1 = new PointLight(0xc4c4c4, 500000);\n    light1.position.set(0, 300, 500);\n    scene.add(light1);\n    const light2 = new PointLight(0xc4c4c4, 500000);\n    light2.position.set(500, 100, 0);\n    scene.add(light2);\n    const light3 = new PointLight(0xc4c4c4, 500000);\n    light3.position.set(0, 100, -500);\n    scene.add(light3);\n    const light4 = new PointLight(0xc4c4c4, 50000);\n    light4.position.set(-500, 300, 500);\n    scene.add(light4);\n\n    // Carregando o modelo GLTF\n    const gltfLoader = new GLTFLoader();\n    gltfLoader.load('../assets/bee_gltf.glb', gltf => {\n      const model = gltf.scene;\n      model.scale.set(150, 150, 150); // Defina a escala do objeto\n      model.position.y = -50;\n      scene.add(model);\n      if (gltf.animations && gltf.animations.length > 0) {\n        console.log('Animações encontradas:', gltf.animations);\n\n        // Adicionando animação\n        const mixer = new THREE.AnimationMixer(model);\n        gltf.animations.forEach(clip => {\n          mixer.clipAction(clip).play();\n        });\n        const animate = () => {\n          requestAnimationFrame(animate);\n          controls.update();\n          mixer.update(0.01); // Atualize a animação\n          renderer.render(scene, camera);\n        };\n        animate();\n      } else {\n        console.log('Nenhuma animação encontrada no modelo.');\n      }\n    });\n    window.addEventListener('resize', onWindowResize, false);\n    function onWindowResize() {\n      camera.aspect = window.innerWidth / window.innerHeight;\n      camera.updateProjectionMatrix();\n      renderer.setSize(window.innerWidth, window.innerHeight);\n    }\n  }\n};","map":{"version":3,"names":["THREE","OrbitControls","PointLight","GLTFLoader","name","mounted","container","$refs","threeContainer","console","error","renderer","WebGLRenderer","setSize","window","innerWidth","innerHeight","appendChild","domElement","scene","Scene","background","Color","camera","PerspectiveCamera","rotation","y","Math","PI","position","x","z","controls","update","light1","set","add","light2","light3","light4","gltfLoader","load","gltf","model","scale","animations","length","log","mixer","AnimationMixer","forEach","clip","clipAction","play","animate","requestAnimationFrame","render","addEventListener","onWindowResize","aspect","updateProjectionMatrix"],"sources":["/home/ramon/repositorios/hatedinthenation/src/App.vue"],"sourcesContent":["<template>\n  <div>\n      <div ref=\"threeContainer\" style=\"width: 100%; height: 100vh;\"></div>\n  </div>\n</template>\n\n<script>\nimport * as THREE from 'three';\nimport { OrbitControls } from 'three/examples/jsm/controls/OrbitControls.js';\nimport { PointLight } from 'three';\n\nimport { GLTFLoader } from 'three/examples/jsm/loaders/GLTFLoader.js';\n\nexport default {\nname: 'ThreeComponent',\nmounted() {\n  const container = this.$refs.threeContainer;\n  if (!container) {\n    console.error('Element with ref \"threeContainer\" not found');\n    return;\n  }\n\n  const renderer = new THREE.WebGLRenderer();\n  renderer.setSize(window.innerWidth, window.innerHeight);\n  container.appendChild(renderer.domElement);\n\n  const scene = new THREE.Scene();\n  scene.background = new THREE.Color(0xdddddd);\n\n  const camera = new THREE.PerspectiveCamera(45, window.innerWidth / window.innerHeight, 1, 2000);\n  camera.rotation.y = (45 / 100) * Math.PI;\n  camera.position.x = 800;\n  camera.position.y = 100;\n  camera.position.z = 1000;\n\n  const controls = new OrbitControls(camera, renderer.domElement);\n  controls.update();\n\n  // Adicionando luzes à cena\n  const light1 = new PointLight(0xc4c4c4, 500000);\n  light1.position.set(0, 300, 500);\n  scene.add(light1);\n\n  const light2 = new PointLight(0xc4c4c4, 500000);\n  light2.position.set(500, 100, 0);\n  scene.add(light2);\n\n  const light3 = new PointLight(0xc4c4c4, 500000);\n  light3.position.set(0, 100, -500);\n  scene.add(light3);\n\n  const light4 = new PointLight(0xc4c4c4, 50000);\n  light4.position.set(-500, 300, 500);\n  scene.add(light4);\n\n // Carregando o modelo GLTF\n  const gltfLoader = new GLTFLoader();\n  gltfLoader.load('../assets/bee_gltf.glb', (gltf) => {\n    const model = gltf.scene;\n    model.scale.set(150, 150, 150); // Defina a escala do objeto\n    model.position.y = -50;\n    scene.add(model);\n\n    if (gltf.animations && gltf.animations.length > 0) {\n      console.log('Animações encontradas:', gltf.animations);\n\n      // Adicionando animação\n      const mixer = new THREE.AnimationMixer(model);\n      gltf.animations.forEach((clip) => {\n        mixer.clipAction(clip).play();\n      });\n\n      const animate = () => {\n        requestAnimationFrame(animate);\n        controls.update();\n        mixer.update(0.01); // Atualize a animação\n        renderer.render(scene, camera);\n      };\n\n      animate();\n    } else {\n      console.log('Nenhuma animação encontrada no modelo.');\n    }\n  });\n  window.addEventListener('resize', onWindowResize, false);\n  function onWindowResize() {\n    camera.aspect = window.innerWidth / window.innerHeight;\n    camera.updateProjectionMatrix();\n    renderer.setSize(window.innerWidth, window.innerHeight);\n  }\n}\n};\n</script>\n\n<style scoped>\ndiv {\n  width: 100%;\n  height: 100%;\n}\n</style>\n"],"mappings":"AAOA,OAAO,KAAKA,KAAI,MAAO,OAAO;AAC9B,SAASC,aAAY,QAAS,8CAA8C;AAC5E,SAASC,UAAS,QAAS,OAAO;AAElC,SAASC,UAAS,QAAS,0CAA0C;AAErE,eAAe;EACfC,IAAI,EAAE,gBAAgB;EACtBC,OAAOA,CAAA,EAAG;IACR,MAAMC,SAAQ,GAAI,IAAI,CAACC,KAAK,CAACC,cAAc;IAC3C,IAAI,CAACF,SAAS,EAAE;MACdG,OAAO,CAACC,KAAK,CAAC,6CAA6C,CAAC;MAC5D;IACF;IAEA,MAAMC,QAAO,GAAI,IAAIX,KAAK,CAACY,aAAa,CAAC,CAAC;IAC1CD,QAAQ,CAACE,OAAO,CAACC,MAAM,CAACC,UAAU,EAAED,MAAM,CAACE,WAAW,CAAC;IACvDV,SAAS,CAACW,WAAW,CAACN,QAAQ,CAACO,UAAU,CAAC;IAE1C,MAAMC,KAAI,GAAI,IAAInB,KAAK,CAACoB,KAAK,CAAC,CAAC;IAC/BD,KAAK,CAACE,UAAS,GAAI,IAAIrB,KAAK,CAACsB,KAAK,CAAC,QAAQ,CAAC;IAE5C,MAAMC,MAAK,GAAI,IAAIvB,KAAK,CAACwB,iBAAiB,CAAC,EAAE,EAAEV,MAAM,CAACC,UAAS,GAAID,MAAM,CAACE,WAAW,EAAE,CAAC,EAAE,IAAI,CAAC;IAC/FO,MAAM,CAACE,QAAQ,CAACC,CAAA,GAAK,EAAC,GAAI,GAAG,GAAIC,IAAI,CAACC,EAAE;IACxCL,MAAM,CAACM,QAAQ,CAACC,CAAA,GAAI,GAAG;IACvBP,MAAM,CAACM,QAAQ,CAACH,CAAA,GAAI,GAAG;IACvBH,MAAM,CAACM,QAAQ,CAACE,CAAA,GAAI,IAAI;IAExB,MAAMC,QAAO,GAAI,IAAI/B,aAAa,CAACsB,MAAM,EAAEZ,QAAQ,CAACO,UAAU,CAAC;IAC/Dc,QAAQ,CAACC,MAAM,CAAC,CAAC;;IAEjB;IACA,MAAMC,MAAK,GAAI,IAAIhC,UAAU,CAAC,QAAQ,EAAE,MAAM,CAAC;IAC/CgC,MAAM,CAACL,QAAQ,CAACM,GAAG,CAAC,CAAC,EAAE,GAAG,EAAE,GAAG,CAAC;IAChChB,KAAK,CAACiB,GAAG,CAACF,MAAM,CAAC;IAEjB,MAAMG,MAAK,GAAI,IAAInC,UAAU,CAAC,QAAQ,EAAE,MAAM,CAAC;IAC/CmC,MAAM,CAACR,QAAQ,CAACM,GAAG,CAAC,GAAG,EAAE,GAAG,EAAE,CAAC,CAAC;IAChChB,KAAK,CAACiB,GAAG,CAACC,MAAM,CAAC;IAEjB,MAAMC,MAAK,GAAI,IAAIpC,UAAU,CAAC,QAAQ,EAAE,MAAM,CAAC;IAC/CoC,MAAM,CAACT,QAAQ,CAACM,GAAG,CAAC,CAAC,EAAE,GAAG,EAAE,CAAC,GAAG,CAAC;IACjChB,KAAK,CAACiB,GAAG,CAACE,MAAM,CAAC;IAEjB,MAAMC,MAAK,GAAI,